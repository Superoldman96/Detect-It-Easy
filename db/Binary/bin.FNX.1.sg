meta("format", "FNX");

function detect() {
    if (Binary.compare("'adgames'")) {
        var copyrightYear = Binary.getString(17, 4).trim();

        if (/^[12]\d{3}$/.test(copyrightYear)) 
            sVersion = copyrightYear;
            bDetected = true;
        }
            return result();
    }

