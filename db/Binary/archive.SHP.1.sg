meta("archive", "Shaped (.SHP)");

function detect() {
    if (Binary.compare("'Shaped'")) {

        var copyrightYear = Binary.getString(28, 4).trim();
        
        if (/^[12]\d{3}$/.test(copyrightYear)) {
            sVersion = copyrightYear;
            bDetected = true;
        }
    }
        return result();
}

